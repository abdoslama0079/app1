@Library('first-lib') _
pipeline {
    agent any
    environment {
        REPO = "app1-flask"
        TAG   = "${BUILD_NUMBER}"
        PORT = "5000"
    }

    stages {
        stage('build') {
            steps {
               buildImage() 
            }
        }
        stage('deploy') {
            steps {
deployApp()
            }
        }
    }
}
